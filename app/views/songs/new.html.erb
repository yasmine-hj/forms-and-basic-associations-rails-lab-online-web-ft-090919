<h1>Create a Song</h1>

<%= form_for @song do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br />
    <%= f.text_field :artist_name %>
  <br />
  <%= f.collection_select :genre_id, Genre.all, :id, :name %>
  <br />
  <%= f.fields_for :notes do |note_form| %>
    <%= note_form.label :note %>
    <%= note_form.text_area :content, rows: 5, cols: 50 %>
    <br/>
  <% end %>
  <br/>


<%= f.submit %>
<% end %>


